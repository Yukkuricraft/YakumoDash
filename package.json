{
  "name": "yakumo-dash",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve:local": "ng serve --verbose --host 0.0.0.0 --port 25699 --disable-host-check --configuration local",
    "serve:dev": "ng serve --verbose --host 0.0.0.0 --port 25699 --disable-host-check --configuration development",
    "build:dev": "ng build --configuration development",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "lint": "yarn prettier -c .",
    "lint:fix": "yarn prettier -w ."
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "private": true,
  "dependencies": {
    "@abacritt/angularx-social-login": "^1.2.1",
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "@ngrx/component": "^15.4.0",
    "@ngrx/effects": "^15.4.0",
    "@ngrx/store": "^15.4.0",
    "@ngrx/store-devtools": "^15.4.0",
    "class-transformer": "^0.5.1",
    "eslint": ">=5.16.0",
    "eslint-plugin-ngrx": "^2.1.3",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.33.0",
    "ng-terminal": "^5.2.0",
    "ngx-monaco-editor-v2": "^15.0.1",
    "ngx-socket-io": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "xterm": "^5.0.0",
    "zone.js": "~0.11.4"
  },
  "archive": {
    "ngrx-store-localstorage": "^15.4.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@angular-eslint/builder": "^15.0.2",
    "@angular-eslint/eslint-plugin-template": "^15.0.2",
    "@angular-eslint/template-parser": "^15.0.2",
    "@angular-devkit/build-angular": "^15.2.11",
    "@angular-eslint/eslint-plugin": "^15.0.2",
    "@angular/cli": "^15.2.11",
    "@angular/compiler-cli": "^15.2.10",
    "@types/jasmine": "~3.10.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^12.11.1",
    "eslint-config-google": "^0.14.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "2.7.1",
    "typescript": "~4.8.4"
  },
  "overrides": {
    "ngx-monaco-editor-v2": {
      "@angular/common": "$@angular/common",
      "@angular/core": "$@angular/core",
      "monaco-editor": "^0.33.0"
    }
  }
