{
  "name": "yakumo-dash",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve:local": "ng serve --verbose --host 0.0.0.0 --port 25699 --disable-host-check --configuration local",
    "serve:dev": "ng serve --verbose --host 0.0.0.0 --port 25699 --disable-host-check --configuration development",
    "build:dev": "ng build --configuration development",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "lint": "yarn prettier -c .",
    "lint:fix": "yarn prettier -w ."
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "private": true,
  "dependencies": {
    "@abacritt/angularx-social-login": "^2.1.0",
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.14",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/material": "^16.2.14",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@ngrx/component": "^16.3.0",
    "@ngrx/effects": "^16.3.0",
    "@ngrx/store": "^16.3.0",
    "@ngrx/store-devtools": "^16.3.0",
    "class-transformer": "^0.5.1",
    "eslint": ">=5.16.0",
    "eslint-plugin-ngrx": "^2.1.3",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.33.0",
    "ng-terminal": "^5.2.0",
    "ngx-monaco-editor-v2": "^16.0.1",
    "ngrx-store-localstorage": "^16.1.0",
    "ngx-socket-io": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "xterm": "^5.0.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@angular-eslint/builder": "^16.0.2",
    "@angular-eslint/eslint-plugin-template": "^16.0.2",
    "@angular-eslint/template-parser": "^16.0.2",
    "@angular-devkit/build-angular": "^16.2.14",
    "@angular-eslint/eslint-plugin": "^16.0.2",
    "@angular/cli": "^16.2.14",
    "@angular/compiler-cli": "^16.2.12",
    "@types/jasmine": "~3.10.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^12.11.1",
    "eslint-config-google": "^0.14.0",
    "jasmine-core": "~4.0.0",
    "prettier": "2.7.1",
    "typescript": "~5.1.0"
  }
