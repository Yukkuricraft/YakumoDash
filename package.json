{
  "name": "yakumo-dash",
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "serve:local": "ng serve --verbose --host 0.0.0.0 --port 25699 --disable-host-check --configuration local",
    "serve:dev": "ng serve --verbose --host 0.0.0.0 --port 25699 --disable-host-check --configuration development",
    "build:dev": "ng build --configuration development",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "lint": "yarn prettier -c .",
    "lint:fix": "yarn prettier -w ."
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "private": true,
  "dependencies": {
    "@abacritt/angularx-social-login": "2.1.0",
    "@angular/animations": "^17.3.12",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@ngrx/component": "^17.2.0",
    "@ngrx/effects": "^17.2.0",
    "@ngrx/store": "^17.2.0",
    "@ngrx/store-devtools": "^17.2.0",
    "@xterm/addon-attach": "^0.11.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "class-transformer": "^0.5.1",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.44.0",
    "ngrx-store-localstorage": "^17.0.0",
    "ngx-monaco-editor-v2": "17.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular-eslint/builder": "^17.0.2",
    "@angular-eslint/eslint-plugin": "^17.0.2",
    "@angular-eslint/eslint-plugin-template": "^17.0.2",
    "@angular-eslint/template-parser": "^17.0.2",
    "@angular/cli": "^17.3.8",
    "@angular/compiler-cli": "^17.3.12",
    "@ngrx/eslint-plugin": "17.2.0",
    "@types/jasmine": "~3.10.0",
    "@types/lodash-es": "^4.14.182",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.56.0",
    "jasmine-core": "~4.0.0",
    "prettier": "2.7.1",
    "typescript": "~5.4.5"
  }
}
